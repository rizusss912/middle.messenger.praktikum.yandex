{
    "extends": "xo",
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "rules": {
        "max-len": [2, 100],
        "@typescript-eslint/no-unused-vars": 2,
        "camelcase": ["error", {"allow": ["first_name", "last_name", "second_name", "display_name"]}],
        "no-mixed-spaces-and-tabs": 0
    },
    "env": {
        "browser": true
    }
} 